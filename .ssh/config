Include conf.d/*.conf

Host *
  Protocol 2
  HashKnownHosts yes
  VisualHostKey yes
  PasswordAuthentication no
  StrictHostKeyChecking ask
  #ControlMaster auto
  #ControlPath ~/.ssh/master-%r@%h:%p
  #ControlPersist 5m
  #ServerAliveInterval 60
  UseRoaming no
  #IdentitiesOnly yes
  #IdentityFile ~/.ssh/id_rsa_yubikey.pub

